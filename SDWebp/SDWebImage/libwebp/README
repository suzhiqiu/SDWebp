	tvOS 10.0                     	-sdk appletvos10.0

tvOS Simulator SDKs:
	Simulator - tvOS 10.0         	-sdk appletvsimulator10.0

watchOS SDKs:
	watchOS 3.1                   	-sdk watchos3.1

watchOS Simulator SDKs:
